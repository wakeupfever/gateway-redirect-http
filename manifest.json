{
  "manifest_version": 3,
  "name": "gateway-redirect",
  "description": "gateway-redirect-http",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "gateway-redirect.png"
  },
  "externally_connectable": {
    "matches": ["<all_urls>"]
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "permissions": ["activeTab", "tabs", "storage"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "history",
    "storage",
    "scripting"
  ]
}